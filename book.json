{
  "plugins": [
    "sharing",
    "custom-favicon",
    "gtm",
    "custom-js-css"
  ],
  "pluginsConfig": {
    "theme-bandwidth": {
      "languages": [
        {
          "lang": "js",
          "name": "Js",
          "default": true
        },
        {
          "lang": "csharp",
          "name": "C#"
        },
        {
          "lang": "ruby",
          "name": "ruby"
        },
        {
          "lang": "bash",
          "name": "cURL"
        }
      ]
    },
    "layout": {
      "headerPath": "layouts/header.html"
    },
    "gtm": {
      "token": "GTM-P2JT7Z",
      "virtualPageViews": true
    },
    "favicon": "favicon/favicon.ico",
    "custom-js-css": {
      "js": [
        "./_theme/tree-shaper.js"
      ],
      "css": [
        "./_theme/tree-shaper.css"
      ]

    }
  }
}