    "usabilla": {
      "usabillaId": "c590d7d0e7ac"
    },
    "favicon": "favicon/favicon.ico",
    "sharing": {
      "facebook": false,
      "twitter": false,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "google",
        "twitter",
        "weibo",
        "instapaper"
